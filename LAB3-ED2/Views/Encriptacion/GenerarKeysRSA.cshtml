@{
    Layout = "~/Views/Shared/_Layout.cshtml"; ViewBag.Title = "Generate Keys";
}

<h2>Generar Llaves para Cifrado RSA</h2>
@using (Html.BeginForm("GenerarKeysRSA", "Encriptacion", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <h2></h2>
    <h4>Ingrese dos numeros primos</h4>
    <input type="number" name="NumberP">
    <input type="number" name="NumberQ">
    <p>
        <input type="submit" value="Importar" class="btn btn-default" />
    </p>
}
<p>@Html.ActionLink("Menu RSA", "CifradoRSA", "Encriptacion")</p>
